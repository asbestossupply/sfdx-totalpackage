{
  "name": "sfdx-totalpackage",
  "description": "Converts *all* the source in a Salesforce DX project into metadata that you can deploy using Metadata API",
  "version": "0.0.0",
  "author": "Noah Blumenthal",
  "bugs": "https://github.com/hackerhasid/sfdx-totalpackage/issues",
  "dependencies": {
    "@oclif/command": "^1.4.15",
    "@oclif/config": "^1.6.13",
    "@oclif/errors": "^1.0.6",
    "@salesforce/command": "^0.1.1",
    "fs-extra": "^5.0.0",
    "tmp": "0.0.33",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.6",
    "@oclif/plugin-help": "^1.2.5",
    "@oclif/test": "^1.0.5",
    "@oclif/tslint": "^1.1.0",
    "@salesforce/dev-config": "^1.0.4",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.6",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.1.1",
    "sinon": "^4.5.0",
    "ts-node": "5",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages"
  ],
  "homepage": "https://github.com/hackerhasid/sfdx-totalpackage",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "hackerhasid/sfdx-totalpackage",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepare": "yarn run build && oclif-dev manifest",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
